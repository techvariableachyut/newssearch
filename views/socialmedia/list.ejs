            <a href="#">Admin</a>
          </li>
          <li class="breadcrumb-item active">List all Social Media App</li>
          <!-- Breadcrumb Menu-->
         
        </ol>
        <div class="container-fluid">

        <div class="row">
              <div class="col-lg-12">
                <div class="card">
                  <div class="card-header">
                    <i class="fa fa-align-justify"></i>All Social Media Apps</div>
                  <div class="card-body">
                    <table class="table table-responsive-sm table-bordered table-striped table-sm">
                      <thead>


                        <tr>
                          <th>App Id</th>
                          <th>Site</th>
                          <th>App Name</th>
                          <th>Status</th>
                        </tr>
                      </thead>
                      <tbody>
                        
                          <% for(var i=0; i < data.length; i++) { %>
                          <tr>
                              <td><a href="/social-media/app/<%= data[i].id %>"><%= data[i].id %></a></td>
                              <td><%= data[i].site %></td>
                              <td><%= data[i].appname %></td>
                              
                              <td>
                                <span class="badge badge-success">Active</span>
                              </td>
                            </tr>
                          <% } %>     
                        
                      </tbody>
                    </table>
